<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.shane.ecotreck">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ecotechy"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon,android:theme">
        <activity android:name=".ecotreck">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".codeTravel"
            android:label="@string/app_name"
            android:theme="@style/AppTheme1"></activity>
        <activity
            android:name=".codeTreck"
            android:label="@string/app_name"
            android:theme="@style/AppTheme1"></activity>
        <activity
            android:name=".OutOfBox"
            android:label="@string/app_name"
            android:theme="@style/AppTheme1"></activity>
        <activity
            android:name=".openHack"
            android:label="@string/app_name"
            android:theme="@style/AppTheme1"></activity>
    </application>

</manifest>